		if (parentDir === undefined || parentDir === null) {
			parentDir = '';
		}
		$('#new-project-parent').val(parentDir);

		// if spaces, value is the tab size (2, 4, 8), otherwise just say 'tabs'
		let tabchar;
		if (localStorage.getItem('tabChar') == 'tabs') {
			tabchar = 'tabs';
		} else if (localStorage.getItem('tabSize') == undefined) {
			tabchar = '4';
		} else {
			tabchar = localStorage.getItem('tabSize');
		}
		$('#new-project-tabchar').val(tabchar);

		$('#new_project').modal();
	}

	$('#new-project-form').submit(() => {
		if ($('#new-project-name').val().trim() == '') {
			$('#new-project-name').val('Untitled');
		}
		return true;
	});

	$('#create-new-project-btn').click(() => {
		$('#new-project-form').submit();
	});

	$('#new-project-name').keydown((e) => {
		if (e.key == 'Enter') {
			e.preventDefault();
			e.stopPropagation();
		}
	});

	$('#lang-dropdown a').click((e) => {
		$('#programminglist').val(e.currentTarget.dataset.lang);
		$('#lang-dropdown-title').html(e.currentTarget.innerHTML);
	})</script>  <div class="modal fade" id="share_project" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static"><div class="modal-dialog modal-dialog-centered" role="document"><div class="modal-content theme-text-color theme-bg-color"><div class="modal-header"><h5 class="modal-title">Sharing for <b id="share-modal-name">Project Name</b></h5><button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button></div><div class="modal-body"><share-settings id="share-modal-share-settings"></share-settings></div><div class="modal-footer"><button id="share-modal-confirm" class="btn btn-primary" data-dismiss="modal">OK</button> <button class="btn btn-secondary" data-dismiss="modal">Cancel</button></div></div></div></div><script>function openShareProjectDialog(projectData) {
